{
    "pack":{
        "type": "tensor_from_image",
        "image": "image",
        "transforms": [
            {
                "type": "image_to_image",
                "name": "center_crop"
            },
            {
                "type": "image_to_image",
                "name": "scale",
                "width": 224,
                "height": 224
            },
            {
                "type": "image_to_tensor",
                "name": "rgb_norm",
                "mean": [0.485, 0.456, 0.406],
                "std": [0.229, 0.224, 0.225]
            }
        ]
    },
    "unpack":{
        "type": "argmax",
        "dtype": "float",
        "key": "maxIdx",
        "valueKey": "confidence"
    }
}